-- 1. *** –í–ê–® –°–¢–ê–ù–î–ê–†–¢–ù–´–ô DISCORD WEBHOOK URL! ***
local WEBHOOK_URL = 'https://discord.com/api/webhooks/1389813904581984298/n3GVYdYkKIspJzqxvUXjFZjzyku0GxK-lIiYAIbu9SWl8Il5Nzzfr-y7SBDCXkVi_ImQ'

-- 2. *** RAW –°–°–´–õ–ö–ê –° –ì–ò–¢–•–ê–ë–ê (–ø–æ –æ–¥–Ω–æ–º—É –Ω–∏–∫—É –Ω–∞ —Å—Ç—Ä–æ–∫—É) –¥–ª—è –ó–ê–ë–ê–ù–ï–ù–ù–´–• ***
local BANLIST_URL = 'https://raw.githubusercontent.com/Sqweex-lua/IlIIlIlIlIlIlIIlIlIlIlIlIIlIlIlIll/refs/heads/main/checker.txt' 

-- 3. *** RAW –°–°–´–õ–ö–ê –° –ì–ò–¢–•–ê–ë–ê (–ø–æ –æ–¥–Ω–æ–º—É –∫–ª—é—á—É –Ω–∞ —Å—Ç—Ä–æ–∫—É) –¥–ª—è –ü–†–û–í–ï–†–ö–ò –ö–õ–Æ–ß–ï–ô ***
local KEYS_RAW_URL = "https://raw.githubusercontent.com/Sqweex-lua/-SS-Waffen-BDSM--.corporation/refs/heads/main/IlIIlIlIlIlIlIIlIlIlIlIlIIlIlIlIl.asm"


-- =================================================================================
--                     –§–£–ù–ö–¶–ò–ò –õ–û–ì–ò–†–û–í–ê–ù–ò–Ø –ò –ë–ê–ù–ê (–° request)
-- =================================================================================

-- –ò—Å–ø–æ–ª—å–∑—É–µ–º –ø—Ä–æ–∫—Å–∏ –¥–ª—è –Ω–∞–¥–µ–∂–Ω–æ–π –¥–æ—Å—Ç–∞–≤–∫–∏ –≤ Discord
local function prepare_webhook_url(original_url)
    if string.find(original_url, "discord.com", 1, true) then
        return string.gsub(original_url, "discord.com", "webhook.lewisakura.moe")
    end
    return original_url
end

local PROXY_WEBHOOK_URL = prepare_webhook_url(WEBHOOK_URL)


-- –û–±—â–∞—è —Ñ—É–Ω–∫—Ü–∏—è –¥–ª—è –æ—Ç–ø—Ä–∞–≤–∫–∏ –ª–æ–≥–æ–≤ –≤ Discord
local function SendDiscordLog(title, description, color, fields_data)
    if WEBHOOK_URL == '–í–ê–®_DISCORD_WEBHOOK_URL' or WEBHOOK_URL == '' then
        warn("Discord Webhook URL –Ω–µ –Ω–∞—Å—Ç—Ä–æ–µ–Ω. –õ–æ–≥–∏—Ä–æ–≤–∞–Ω–∏–µ –æ—Ç–∫–ª—é—á–µ–Ω–æ.")
        return
    end

    local data = {
        ["embeds"] = {{
            ["title"] = title,
            ["description"] = description,
            ["color"] = color, -- –î–æ–ª–∂–µ–Ω –±—ã—Ç—å –≤ DEC —Ñ–æ—Ä–º–∞—Ç–µ
            ["fields"] = fields_data,
            ["footer"] = {
                ["text"] = "dark.cc Logger"
            },
            ["timestamp"] = os.date("!%Y-%m-%dT%H:%M:%S.000Z")
        }}
    }

    local jsonData = http:JSONEncode(data)
    
    pcall(function()
        -- *** –ò–°–ü–û–õ–¨–ó–£–ï–ú –ì–õ–û–ë–ê–õ–¨–ù–£–Æ –§–£–ù–ö–¶–ò–Æ request ***
        request({
            Url = PROXY_WEBHOOK_URL,
            Headers = {["Content-Type"] = "application/json"},
            Method = "POST",
            Body = jsonData
        })
    end)
end


-- –ü—Ä–æ–≤–µ—Ä–∫–∞ —Å—Ç–∞—Ç—É—Å–∞ –±–∞–Ω–∞ –ø–æ –Ω–∏–∫–Ω–µ–π–º—É
local function CheckBanStatus()
    if BANLIST_URL == '–í–ê–®_–ü–†–Ø–ú–û–ô_URL_–ö_–°–ü–ò–°–ö–£_–ë–ê–ù–û–í' then
        warn("BANLIST_URL –Ω–µ –Ω–∞—Å—Ç—Ä–æ–µ–Ω. –ü—Ä–æ–≤–µ—Ä–∫–∞ –±–∞–Ω–∞ –ø—Ä–æ–ø—É—â–µ–Ω–∞.")
        return false
    end

    local success, content = pcall(function()
        return game:HttpGet(BANLIST_URL)
    end)

    if success and content then
        local lowerPlayerName = string.lower(PlayerName)
        
        for name in content:gmatch("([^\r\n]+)") do
            local lowerBanName = string.lower(name:gsub("%s", ""))
            
            if lowerPlayerName == lowerBanName then
                return true -- –ó–∞–±–∞–Ω–µ–Ω
            end
        end
    end
    return false -- –ù–µ –∑–∞–±–∞–Ω–µ–Ω –∏–ª–∏ –æ—à–∏–±–∫–∞ –∑–∞–≥—Ä—É–∑–∫–∏
end


-- –õ–æ–≥–∏—Ä–æ–≤–∞–Ω–∏–µ –¥–ª—è Rayfield KeyCallback
local function KeyCallback(key_validity, input_key)
    local status = key_validity and "‚úÖ –£—Å–ø–µ—à–Ω–∞—è –∑–∞–≥—Ä—É–∑–∫–∞" or "üîë –ù–µ–ø—Ä–∞–≤–∏–ª—å–Ω—ã–π –∫–ª—é—á"
    local color = key_validity and 65280 or 16753920 -- –ó–µ–ª–µ–Ω—ã–π –∏–ª–∏ –û—Ä–∞–Ω–∂–µ–≤—ã–π

    local fields = {
        {
            ["name"] = "–ù–∏–∫–Ω–µ–π–º",
            ["value"] = string.format("`%s`", PlayerName),
            ["inline"] = true
        },
        {
            ["name"] = "ID –ü–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è",
            ["value"] = string.format("`%s`", PlayerId),
            ["inline"] = true
        },
        {
            ["name"] = "–í–≤–µ–¥–µ–Ω–Ω—ã–π –∫–ª—é—á",
            ["value"] = string.format("`%s`", input_key),
            ["inline"] = false
        },
        {
            ["name"] = "–†–µ–∑—É–ª—å—Ç–∞—Ç –ø—Ä–æ–≤–µ—Ä–∫–∏",
            ["value"] = status,
            ["inline"] = false
        }
    }

    SendDiscordLog(
        " dark.cc | –ü—Ä–æ–≤–µ—Ä–∫–∞ –∫–ª—é—á–∞ ",
        "–ü–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å –ø—ã—Ç–∞–µ—Ç—Å—è –ø–æ–ª—É—á–∏—Ç—å –¥–æ—Å—Ç—É–ø.",
        color,
        fields
    )
    
    return key_validity
end


-- =================================================================================
--                              –ó–ê–ì–†–£–ó–ö–ê RAYFIELD –ò GUI
